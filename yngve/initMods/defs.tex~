%%% environments

\newenvironment{Proof}{{\bf Proof.}}{\nopagebreak\finish}
\newenvironment{Proofs}{{\bf Proof sketch.}}{\nopagebreak\finish}
\newcommand{\finish}{\hspace*{\fill}\nopagebreak 
     \raisebox{-1pt}{$\Box$}\par\addvspace{1.5ex}\noindent}

\newcounter{CLAIM}[section]
\def\theCLAIM{\arabic{section}.\arabic{CLAIM}}

\newtheorem{Claim}[CLAIM]{? Claim}   %[section]
\newtheorem{Definition}[CLAIM]{Definition} %[section]
\newtheorem{Fact}[CLAIM]{Fact}       %[section]
\newtheorem{Prop}[CLAIM]{Claim}       %[section]
\newtheorem{Lemma}[CLAIM]{Lemma}     %[section]
\newtheorem{Example}[CLAIM]{Example} %[section]
\newtheorem{Theorem}[CLAIM]{Theorem} %[section]
\newtheorem{Corollary}[CLAIM]{Corollary}   %[section]

\newcommand{\MyLPar}{\parsep -.2ex plus.2ex minus.2ex\itemsep\parsep 
\vspace{-\topsep}\vspace{.5ex}}

\newenvironment{enum}{\begin{enumerate}\MyLPar}{\end{enumerate}}
\newenvironment{ite}{\begin{itemize}\MyLPar}{\end{itemize}}

